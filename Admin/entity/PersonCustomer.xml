<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-2.1.xsd">
    <entity entity-name="PersonCustomer" package="com.liou" use="transactional">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="cardNumber" type="text-medium" ><description>会员卡号</description></field>
        <field name="memberStatus" type="text-medium"><description>会员状态</description></field>
        <field name="memberShip" type="text-medium"><description>会籍顾问</description></field>
        <field name="joinDate" type="date"><description>入会时间</description></field>
        <field name="totalTimes" type="number-integer"><description>进店总次数</description></field>
        <field name="status" type="text-medium"><description>状态</description></field>
        <field name="photo" type="text-medium"><description>照片</description></field>
        <field name="vein" type="text-medium"><description>指静脉</description></field>
        <field name="customerFrom" type="text-medium"><description>客户来源</description></field>
        <field name="customerClasses" type="text-medium"><description>客户分类</description></field>
        <field name="occupation" type="text-medium"><description>职业</description></field>
        <field name="customerType" type="text-medium"><description>会员类型</description></field>
        <field name="address" type="text-medium"><description>地址</description></field>
        <field name="remark" type="text-medium"><description>备注</description></field>
        
        <relationship type="one" related="mantle.party.Party"/>
<!--         <index name="PERSON_CUSTOMER_PARTY_ID"><index-field name="partyId"/></index> -->
        <seed-data>
         
        </seed-data>
    </entity>
</entities>